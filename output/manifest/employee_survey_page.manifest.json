{
  "source": {
    "file": "./lib/demos/employee_survey_page.dart",
    "pageClass": "EmployeeSurveyPage",
    "cubitClass": "EmployeeCubit",
    "stateClass": "EmployeeState",
    "fileCubit": "lib/cubit/employee_cubit.dart",
    "fileState": "lib/cubit/employee_state.dart"
  },
  "widgets": [
    {
      "widgetType": "Checkbox",
      "key": "employee_07_recommend_checkbox",
      "meta": {
        "valueBinding": "state.recommendCompany"
      }
    },
    {
      "widgetType": "SnackBar",
      "key": "employee_01_expected_success"
    },
    {
      "widgetType": "Radio<int>",
      "key": "employee_06_rating_poor_radio",
      "meta": {
        "valueExpr": "1",
        "groupValueBinding": "state.satisfactionRating"
      }
    },
    {
      "widgetType": "SnackBar",
      "key": "employee_01_expected_fail"
    },
    {
      "widgetType": "TextFormField",
      "key": "employee_02_id_textfield",
      "meta": {
        "inputFormatters": [
          {
            "type": "allow",
            "pattern": "[A-Z0-9\\-]"
          },
          {
            "type": "lengthLimit",
            "max": 15
          }
        ],
        "validatorRules": [
          {
            "condition": "value == null || value.isEmpty",
            "message": "Employee ID is required"
          },
          {
            "condition": "!RegExp(r'^EMP-\\d{5}$').hasMatch(value)",
            "message": "Employee ID must match format: EMP-12345"
          }
        ]
      }
    },
    {
      "widgetType": "Radio<int>",
      "key": "employee_06_rating_fair_radio",
      "meta": {
        "valueExpr": "2",
        "groupValueBinding": "state.satisfactionRating"
      }
    },
    {
      "widgetType": "DropdownButtonFormField<String>",
      "key": "employee_03_department_dropdown",
      "meta": {
        "validatorRules": [
          {
            "condition": "value == null || value.isEmpty",
            "message": "Please select a department"
          }
        ],
        "options": [
          {
            "value": "Engineering",
            "text": "Engineering"
          },
          {
            "value": "Sales",
            "text": "Sales"
          },
          {
            "value": "HR",
            "text": "HR"
          },
          {
            "value": "Marketing",
            "text": "Marketing"
          }
        ]
      }
    },
    {
      "widgetType": "TextFormField",
      "key": "employee_04_email_textfield",
      "meta": {
        "keyboardType": "emailAddress",
        "inputFormatters": [
          {
            "type": "lengthLimit",
            "max": 50
          }
        ],
        "validatorRules": [
          {
            "condition": "value == null || value.isEmpty",
            "message": "Email is required"
          },
          {
            "condition": "!RegExp(r'^[\\w\\.\\-]+@[\\w\\.\\-]+\\.\\w{2,}$')\n                              .hasMatch(value)",
            "message": "Please enter a valid email address"
          }
        ]
      }
    },
    {
      "widgetType": "Radio<int>",
      "key": "employee_06_rating_excellent_radio",
      "meta": {
        "valueExpr": "3",
        "groupValueBinding": "state.satisfactionRating"
      }
    },
    {
      "widgetType": "TextFormField",
      "key": "employee_05_years_textfield",
      "meta": {
        "keyboardType": "number",
        "inputFormatters": [
          {
            "type": "digitsOnly"
          },
          {
            "type": "lengthLimit",
            "max": 2
          }
        ],
        "validatorRules": [
          {
            "condition": "value == null || value.isEmpty",
            "message": "Years of service is required"
          },
          {
            "condition": "years == null || years < 0",
            "message": "Years must be 0 or greater"
          },
          {
            "condition": "years > 50",
            "message": "Years cannot exceed 50"
          }
        ]
      }
    },
    {
      "widgetType": "FormField<int>",
      "key": "employee_06_rating_formfield",
      "meta": {
        "validatorRules": [
          {
            "condition": "value == null",
            "message": "Please select a satisfaction rating"
          }
        ]
      }
    },
    {
      "widgetType": "FormField<bool>",
      "key": "employee_07_recommend_formfield",
      "meta": {
        "validatorRules": [
          {
            "condition": "value == null || !value",
            "message": "You must recommend the company to proceed"
          }
        ]
      }
    },
    {
      "widgetType": "Checkbox",
      "key": "employee_08_training_checkbox",
      "meta": {
        "valueBinding": "state.attendTraining"
      }
    },
    {
      "widgetType": "ElevatedButton",
      "key": "employee_09_end_button"
    }
  ]
}
