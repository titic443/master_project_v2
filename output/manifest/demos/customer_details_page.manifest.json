{
  "source": {
    "file": "lib/demos/customer_details_page.dart",
    "pageClass": "CustomerDetailsPage",
    "cubitClass": "CustomerCubit",
    "stateClass": "CustomerState",
    "fileCubit": "lib/cubit/customer_cubit.dart",
    "fileState": "lib/cubit/customer_state.dart"
  },
  "widgets": [
    {
      "widgetType": "Checkbox",
      "key": "customer_06_agree_terms_checkbox",
      "meta": {
        "valueBinding": "state.agreeToTerms"
      }
    },
    {
      "widgetType": "SnackBar",
      "key": "customer_01_expected_success"
    },
    {
      "widgetType": "Radio<int>",
      "key": "customer_05_age_10_20_radio",
      "meta": {
        "valueExpr": "1",
        "groupValueBinding": "state.ageRange"
      }
    },
    {
      "widgetType": "SnackBar",
      "key": "customer_01_expected_fail"
    },
    {
      "widgetType": "DropdownButtonFormField<String>",
      "key": "customer_01_title_dropdown",
      "meta": {
        "validatorRules": [
          {
            "condition": "value == null || value.isEmpty",
            "message": "Please select a title"
          }
        ],
        "options": [
          {
            "value": "Mr.",
            "text": "Mr."
          },
          {
            "value": "Mrs.",
            "text": "Mrs."
          },
          {
            "value": "Ms.",
            "text": "Ms."
          },
          {
            "value": "Dr.",
            "text": "Dr."
          }
        ]
      }
    },
    {
      "widgetType": "Radio<int>",
      "key": "customer_05_age_30_40_radio",
      "meta": {
        "valueExpr": "2",
        "groupValueBinding": "state.ageRange"
      }
    },
    {
      "widgetType": "TextFormField",
      "key": "customer_02_firstname_textfield",
      "meta": {
        "inputFormatters": [
          {
            "type": "allow",
            "pattern": "[a-zA-Z]"
          },
          {
            "type": "lengthLimit",
            "max": 50
          }
        ],
        "validatorRules": [
          {
            "condition": "value == null || value.isEmpty",
            "message": "First name is required"
          },
          {
            "condition": "!RegExp(r'^[a-zA-Z]{2,}$').hasMatch(value)",
            "message": "First name must contain only letters (minimum 2 characters)"
          }
        ]
      }
    },
    {
      "widgetType": "TextFormField",
      "key": "customer_03_phone_textfield",
      "meta": {
        "inputFormatters": [
          {
            "type": "digitsOnly"
          },
          {
            "type": "lengthLimit",
            "max": 10
          }
        ],
        "validatorRules": [
          {
            "condition": "value == null || value.isEmpty",
            "message": "Phone number is required"
          },
          {
            "condition": "!RegExp(r'^\\d{10}$').hasMatch(value)",
            "message": "Phone number must be exactly 10 digits"
          },
          {
            "condition": "!value.startsWith('0')",
            "message": "Phone number must start with 0"
          },
          {
            "condition": "!RegExp(r'^\\d{10}$').hasMatch(value)",
            "message": "0"
          }
        ]
      }
    },
    {
      "widgetType": "Radio<int>",
      "key": "customer_05_age_40_50_radio",
      "meta": {
        "valueExpr": "3",
        "groupValueBinding": "state.ageRange"
      }
    },
    {
      "widgetType": "TextFormField",
      "key": "customer_04_lastname_textfield",
      "meta": {
        "maxLength": 10,
        "inputFormatters": [
          {
            "type": "allow",
            "pattern": "[a-zA-Z]"
          },
          {
            "type": "lengthLimit",
            "max": 50
          }
        ],
        "validatorRules": [
          {
            "condition": "value == null || value.isEmpty",
            "message": "Last name is required"
          },
          {
            "condition": "!RegExp(r'^[a-zA-Z]{2,}$').hasMatch(value)",
            "message": "Last name must contain only letters (minimum 2 characters)"
          }
        ]
      }
    },
    {
      "widgetType": "FormField<int>",
      "key": "customer_05_age_range_formfield",
      "meta": {
        "validatorRules": [
          {
            "condition": "value == null",
            "message": "Please select an age range"
          }
        ],
        "options": [
          {
            "value": "1",
            "text": "10-20"
          },
          {
            "value": "2",
            "text": "30-40"
          },
          {
            "value": "3",
            "text": "50-60"
          }
        ]
      }
    },
    {
      "widgetType": "FormField<bool>",
      "key": "customer_06_agree_terms_formfield",
      "meta": {
        "validatorRules": [
          {
            "condition": "value == null || !value",
            "message": "You must agree to terms"
          }
        ]
      }
    },
    {
      "widgetType": "Checkbox",
      "key": "customer_07_subscribe_newsletter_checkbox",
      "meta": {
        "valueBinding": "state.subscribeNewsletter"
      }
    },
    {
      "widgetType": "ElevatedButton",
      "key": "customer_08_end_button"
    }
  ]
}
