{
  "source": {
    "file": "lib/demos/product_registration_page.dart",
    "pageClass": "ProductRegistrationPage",
    "cubitClass": "ProductCubit",
    "stateClass": "ProductState",
    "fileCubit": "lib/cubit/product_cubit.dart",
    "fileState": "lib/cubit/product_state.dart"
  },
  "widgets": [
    {
      "widgetType": "Checkbox",
      "key": "product_07_instock_checkbox",
      "meta": {
        "valueBinding": "state.inStock"
      }
    },
    {
      "widgetType": "SnackBar",
      "key": "product_01_expected_success"
    },
    {
      "widgetType": "Radio<int>",
      "key": "product_06_price_0_100_radio",
      "meta": {
        "valueExpr": "1",
        "groupValueBinding": "state.priceRange"
      }
    },
    {
      "widgetType": "SnackBar",
      "key": "product_01_expected_fail"
    },
    {
      "widgetType": "TextFormField",
      "key": "product_02_name_textfield",
      "meta": {
        "inputFormatters": [
          {
            "type": "lengthLimit",
            "max": 100
          }
        ],
        "validatorRules": [
          {
            "condition": "value == null || value.isEmpty",
            "message": "Product name is required"
          },
          {
            "condition": "value.length < 3",
            "message": "Product name must be at least 3 characters"
          }
        ]
      }
    },
    {
      "widgetType": "Radio<int>",
      "key": "product_06_price_100_500_radio",
      "meta": {
        "valueExpr": "2",
        "groupValueBinding": "state.priceRange"
      }
    },
    {
      "widgetType": "DropdownButtonFormField<String>",
      "key": "product_03_category_dropdown",
      "meta": {
        "validatorRules": [
          {
            "condition": "value == null || value.isEmpty",
            "message": "Please select a category"
          }
        ],
        "options": [
          {
            "value": "Electronics",
            "text": "Electronics"
          },
          {
            "value": "Clothing",
            "text": "Clothing"
          },
          {
            "value": "Food",
            "text": "Food"
          },
          {
            "value": "Books",
            "text": "Books"
          }
        ]
      }
    },
    {
      "widgetType": "TextFormField",
      "key": "product_04_sku_textfield",
      "meta": {
        "inputFormatters": [
          {
            "type": "allow",
            "pattern": "[A-Z0-9\\-]"
          },
          {
            "type": "lengthLimit",
            "max": 20
          }
        ],
        "validatorRules": [
          {
            "condition": "value == null || value.isEmpty",
            "message": "SKU is required"
          },
          {
            "condition": "!RegExp(r'^[A-Z0-9\\-]{5,}$').hasMatch(value)",
            "message": "SKU must be uppercase alphanumeric with dash (min 5 chars)"
          }
        ]
      }
    },
    {
      "widgetType": "Radio<int>",
      "key": "product_06_price_500_plus_radio",
      "meta": {
        "valueExpr": "3",
        "groupValueBinding": "state.priceRange"
      }
    },
    {
      "widgetType": "TextFormField",
      "key": "product_05_quantity_textfield",
      "meta": {
        "keyboardType": "number",
        "inputFormatters": [
          {
            "type": "digitsOnly"
          },
          {
            "type": "lengthLimit",
            "max": 6
          }
        ],
        "validatorRules": [
          {
            "condition": "value == null || value.isEmpty",
            "message": "Quantity is required"
          },
          {
            "condition": "qty == null || qty < 1",
            "message": "Quantity must be at least 1"
          },
          {
            "condition": "qty > 999999",
            "message": "Quantity cannot exceed 999999"
          }
        ]
      }
    },
    {
      "widgetType": "FormField<int>",
      "key": "product_06_price_range_formfield",
      "meta": {
        "validatorRules": [
          {
            "condition": "value == null",
            "message": "Please select a price range"
          }
        ]
      }
    },
    {
      "widgetType": "FormField<bool>",
      "key": "product_07_instock_formfield",
      "meta": {
        "validatorRules": [
          {
            "condition": "value == null || !value",
            "message": "Product must be in stock to register"
          }
        ]
      }
    },
    {
      "widgetType": "Checkbox",
      "key": "product_08_featured_checkbox",
      "meta": {
        "valueBinding": "state.featured"
      }
    },
    {
      "widgetType": "ElevatedButton",
      "key": "product_09_end_button"
    }
  ]
}
